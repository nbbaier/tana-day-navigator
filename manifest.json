{
    "manifest_version": 3,
    "name": "Tana Day Navigator",
    "version": "1.0",
    "description": "Adds keyboard shortcuts to navigate between days in Tana",
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
                "https://app.tana.inc/*"
            ]
        }
    ]
}